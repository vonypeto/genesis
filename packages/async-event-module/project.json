{
  "name": "async-event-module",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/async-event-module/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "nx:run-commands",
